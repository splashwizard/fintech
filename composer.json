{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4.1",
        "ext-json": "*",
        "automattic/woocommerce": "^3.0",
        "composer/semver": "^1.4",
        "consoletvs/charts": "^6.5.2",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "fzaninotto/faker": "^1.9",
        "guzzlehttp/guzzle": "^7.2",
        "jeremykenedy/laravel-logger": "^3.8",
        "knox/pesapal": "^1.3",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "laravel/vapor-cli": "^1.12",
        "laravelcollective/html": "^6.0.3",
        "milon/barcode": "^6.0",
        "nwidart/laravel-modules": "^5.0",
        "razorpay/razorpay": "2.*",
        "spatie/flysystem-dropbox": "^1.0",
        "spatie/laravel-activitylog": "^3.2",
        "spatie/laravel-backup": "^6.0.0",
        "spatie/laravel-permission": "^3.0.0",
        "srmklive/paypal": "^1.6",
        "stripe/stripe-php": "^6.10",
        "yajra/laravel-datatables-buttons": "4.*",
        "yajra/laravel-datatables-oracle": "^9.0.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "nunomaduro/collision": "^2.0",
        "beyondcode/laravel-dump-server": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Http/helpers.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    }
}
